<!--pages/billing/index.wxml-->
<view class='container'>
  <view class='number'>
    <text>当前车俩：{{number}}</text>
  </view>
  <view class='time'>
    <view class='time-title'>{{title}}
    </view>
    <view class='time-content'>{{hours}}：{{minutes}}：{{scends}}
    </view>
  </view>
  <view class='endride'>
    <button type='warn' bindtap='endride' disabled='{{disable}}'>结束骑行</button>
    <button type='primary' class='endride-back' bindtap='backMap'>回到地图</button>
  </view>
</view>
